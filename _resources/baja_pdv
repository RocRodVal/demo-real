/* borrar acceso */
DELETE FROM `agent` WHERE `sfid` IN (16450008,19993776);

/* borrar muebles */
DELETE displays_pds FROM displays_pds
INNER JOIN pds ON pds.id_pds = displays_pds.id_pds
WHERE pds.reference IN (16450008,19993776);

/* borrar terminales */
DELETE devices_pds FROM devices_pds
INNER JOIN pds ON pds.id_pds = devices_pds.id_pds
WHERE pds.reference IN (16450008,19993776);

/* borrar pdv */
DELETE FROM `pds` WHERE `reference` IN (16450008,19993776);
